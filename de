import os
import re
import time
for i in os.listdir():
    print(i)
    if i.endswith(".py"):
        with open(i,"r+") as f:
            data = f.read()
        with open(i,"w") as g:
            print(data)
            new_data = re.sub(r"keke","eoms",data)
            print(new_data)
            g.write(new_data)
            time.sleep(0.1)

